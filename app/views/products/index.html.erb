<div class="container-md">
  <div class="row justify-content-md-center">
    <div class="col">
      <h2>Products</h2>
    </div>
    <div class="col-md-auto"></div>
    <div class="col-md-auto">
      <%# Button outputs a path to .../new? %>
      <%= button_to "+", {action: "new", type: "button"},
        {method: "get", class: "btn btn-dark shadow", style: "background-color: #333f65"}  %>
    </div>
  </div>
  <%= react_component("App", {products: @products, prices: @product_prices} ) %>
</div>
